
@{
    ViewBag.Title = "Tipo razones";
}
<div class="box bac">
    <div class="box-header">
        <h3 class="titulo">Información de Clientes</h3>
        <div class="row nomargin">
            <button class="btn btn-md btn-success" onclick="NuevoTipoRazon(event)" id="btn_nuevo_registro">
                <i class="fa fa-plus"> </i>
                <span> Nuevo Cliente</span>
            </button>
        </div>
    </div>
    <div class="box-body bac-table-panel">
        <table id="tableTipoRazones" class="list-table table table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>RTN</th>
                    <th>Nombre</th>
                    <th>Genero</th>
                    <th>Edad</th>
                    <th>Estado Cliente</th>
                    <th>Dirección</th>
                    <th>Tel Casa</th>
                    <th>Celular</th>
                    <th>Correo</th>
                    <th>Acciones</th>
                    <th hidden="hidden"></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
<!-- Modal -->
<div class="modal fade" id="moda_tipo_razones" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel"><span id="theHeader">Agregar/Editar Tipo Razon</span></h4>
            </div>
            <div class="modal-body">
                <input type="hidden" id="hidden_id" />
                <div id="form">
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="identidad">ID</label>
                                <input type="text" class="form-control identidad" id="identidad" name="identidad" attr-message="Es un campo requerido!" placeholder="Número de Identidad" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="rtn">RTN</label>
                                <input type="text" class="form-control rtn" id="rtn" name="rtn" attr-message="Es un campo requerido!" placeholder="RTN" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="primernombre">Primer Nombre</label>
                                <input type="text" class="form-control varchar50" id="primernombre" name="primernombre" attr-message="Es un campo requerido!" placeholder="Primer Nombre del Cliente" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="segundonombre">Segundo Nombre</label>
                                <input type="text" class="form-control varchar50" id="segundonombre" name="segundonombre" attr-message="Es un campo requerido!" placeholder="Segundo Nombre del Cliente" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="primerapellido">Primer Apellido</label>
                                <input type="text" class="form-control varchar50" id="primerapellido" name="primerapellido" attr-message="Es un campo requerido!" placeholder="Primer Apellido del Cliente" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="segundoapellido">Segundo Apellido</label>
                                <input type="text" class="form-control varchar50" id="segundoapellido" name="segundoapellido" attr-message="Es un campo requerido!" placeholder="Segundo Apellido del Cliente" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm" id="genero">
                                <label for="genero">Genero</label>
                                <select class="selectpicker show-menu-arrow requerido" data-live-search="true" title="Seleccione" id="genero" name="genero" attr-message="Es un campo requerido!">
                                    <option value="0">Femenino</option>
                                    <option value="1">Masculino</option>
                                </select>
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="telcasa">Teléfono Resdidencial</label>
                                <input type="text" class="form-control telefonoCelular" id="telcasa" name="telcasa" attr-message="Es un campo requerido!" placeholder="Teléfono Fijo" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="celular">Celular</label>
                                <input type="text" class="form-control telefonoCelular" id="celular" name="celular" attr-message="Es un campo requerido!" placeholder="Celular" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="correo">Correo</label>
                                <input type="text" class="form-control correo" id="correo" name="correo" attr-message="Es un campo requerido!" placeholder="Correo Electrónico" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="fecha">Fecha de Nacimiento</label>
                                <input type="text" class="form-control fecha" id="fecha" name="fecha" attr-message="Es un campo requerido!" placeholder="Seleccione Fecha de Nacimiento" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="estado">Estado</label>
                                <select class="selectpicker show-menu-arrow requerido" data-live-search="true" title="Seleccione" id="estado" name="estado" attr-message="Es un campo requerido!">
                                    <option value="0">ACTIVO</option>
                                    <option value="1">INACTIVO</option>
                                </select>
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm" id="genero">
                                <label for="estcivil">Estado Civil</label>
                                <select class="selectpicker show-menu-arrow requerido" data-live-search="true" title="Seleccione" id="estcivil" name="estcivil" attr-message="Es un campo requerido!">
                                    <option value="0">SOLTERO</option>
                                    <option value="1">CASADO</option>
                                    <option value="1">UNION LIBRE</option>
                                </select>
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="direccion">Dirección</label>
                                <input type="text" class="form-control varchar200" id="direccion" name="direccion" attr-message="Es un campo requerido!" placeholder="Dirección Residencial" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                    </div>

