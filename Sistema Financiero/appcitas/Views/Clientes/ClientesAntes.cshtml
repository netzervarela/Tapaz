                </div>
            </div>


            @*PRIMERA REFERENCIA PERSONAL*@
            <div class="container" style="background-color: #212F3D; color: white; height: 3.9em;">
                <h4 style="margin-top: 1em;">Primera Referencia Personal</h4>
            </div>
            <br />

            <div>
                <div class="col-sm-6">
                    <div class="form-group input-group-sm">
                        <label for="NomRef1">Nombre Completo</label>
                        <input type="text" class="form-control varchar50 requerido" id="NomRef1" attr-message="Es un campo requerido!" name="NomRef1" placeholder="Nombre Completo Referencia 1" />
                        <div class="validation-error"><p></p></div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group input-group-sm">
                        <label for="IdRef1">Identidad</label>
                        <input type="text" class="form-control identidad requerido" id="IdRef1" attr-message="Es un campo requerido!" name="IdRef1" placeholder="Identidad Referencia 1" />
                        <div class="validation-error"><p></p></div>
