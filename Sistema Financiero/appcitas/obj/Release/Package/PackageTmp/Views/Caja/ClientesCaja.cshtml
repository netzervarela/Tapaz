<!-- Modal -->
<div class="modal fade" id="moda_tipo_razones" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel"><span id="theHeader">Agregar/Editar Tipo Razon</span></h4>
            </div>
            <div class="modal-body">
                <input type="hidden" id="hidden_id" />
                <div id="form">
                    <div class="row">
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="identidad">ID</label>
                                <input type="text" class="form-control varchar50" id="identidad" name="identidad" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="rtn">RTN</label>
                                <input type="text" class="form-control varchar50" id="rtn" name="rtn" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="primernombre">Primer Nombre</label>
                                <input type="text" class="form-control varchar50" id="primernombre" name="primernombre" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="segundonombre">Segund Nombre</label>
                                <input type="text" class="form-control varchar50" id="segundonombre" name="segundonombre" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="primerapellido">Primer Apellido</label>
                                <input type="text" class="form-control varchar50" id="primerapellido" name="primerapellido" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="segundoapellido">Segund Apellido</label>
                                <input type="text" class="form-control varchar50" id="segundoapellido" name="segundoapellido" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm" id="genero">
                                <label for="genero">Genero</label>
                                <select class="selectpicker show-menu-arrow requerido" data-live-search="true" title="Seleccione" id="genero" name="genero" attr-message="Es un campo requerido!">
                                    <option value="0">Femenino</option>
                                    <option value="1">Masculino</option>
                                </select>
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="telcasa">Tel Casa</label>
                                <input type="text" class="form-control varchar50" id="telcasa" name="telcasa" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="celular">Celular</label>
                                <input type="text" class="form-control varchar50" id="celular" name="celular" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group input-group-sm">
                                <label for="correo">Correo</label>
                                <input type="text" class="form-control varchar50" id="correo" name="correo" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group input-group-sm">
                                <label for="fecha">Fecha de Nacimiento</label>
                                <input type="text" class="form-control varchar50" id="fecha" name="fecha" attr-message="Es un campo requerido!" />
                                <div class="validation-error"><p></p></div>
                            </div>
                        </div>
                        @*<div class="form-group input-group-sm date col-md-6">
            <label for="fecha">Fecha Nacimiento</label>
            <div class='input-group input-group-sm date' id="fecha">
                <span class="input-group-addon">
                    <span class="fa fa-calendar"></span>
